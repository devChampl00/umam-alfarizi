{
    "version": 2,
    "builds": [
      {
        "src": "next.config.mjs",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/_next/static/(?:[^/]+/pages|chunks|runtime|css|media)/.+",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        }
      },
      {
        "src": "/favicon.ico",
        "dest": "/public/favicon.ico"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/"
      }
    ]
  }
