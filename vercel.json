{
    "version": 2,
    "builds": [
      {
        "src": "next.config.mjs",
        "use": "@vercel/next"
      }
    ],
    "headers": [
      {
        "source": "/_next/static/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/favicon.ico",
        "destination": "/public/favicon.ico"
      }
    ]
}
